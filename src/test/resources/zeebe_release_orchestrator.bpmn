      <zeebe:userTaskForm id="userTaskForm_04ch9d3">{   "schemaVersion": 2,   "components": [     {       "label": "Release version",       "type": "textfield",       "id": "Field_0q04txm",       "key": "release_version",       "description": "MAJOR.MINOR.PATCH-ALPHA (Eg:- 1.3.0-alpha1)"     },     {       "label": "Release date",       "type": "textfield",       "id": "Field_1jl2q1w",       "key": "release_date",       "description": "Format: yyyy-MM-dd'T'HH:mm:ss@ZoneId (Eg:- 2021-10-20T:00:00:00Z)"     },     {       "label": "Code Freeze date",       "type": "textfield",       "id": "Field_0w4n3mw",       "key": "code_freeze_date",       "description": "Format: yyyy-MM-dd'T'HH:mm:ss@ZoneId (Eg:- 2021-10-20T:00:00:00Z)"     },     {       "values": [         {           "label": "Philipp",           "value": "philipp"         },         {           "label": "Chris",           "value": "chris"         },         {           "label": "Peter",           "value": "peter"         },         {           "value": "deepthi",           "label": "Deepthi"         },         {           "label": "Nico",           "value": "nico"         },         {           "label": "Remco",           "value": "remco"         },         {           "label": "Ole",           "value": "ole"         },         {           "label": "Nicolas",           "value": "nicolas"         }       ],       "label": "Release Manager",       "type": "select",       "id": "Field_08k3xs4",       "key": "release_manager"     },     {       "values": [         {           "label": "Major",           "value": "major"         },         {           "label": "Minor",           "value": "minor"         },         {           "label": "Alpha",           "value": "alpha"         },         {           "label": "Patch",           "value": "patch"         }       ],       "label": "The release is",       "type": "radio",       "id": "Field_01mvgma",       "key": "release_type"     }   ],   "type": "default",   "id": "release_collect_data" }</zeebe:userTaskForm>
      <zeebe:userTaskForm id="userTaskForm_1m36ud9">{   "schemaVersion": 2,   "components": [     {       "text": "Remind zeebe team about the code freeze date. Send the following message to #zeebe slack\n\n```\nHi team, The code freeze for release &lt;release_version&gt; is on &lt;code_freeze_date&gt;. \nRemember to merge all necessary PRs before that. Inform &lt;release_manager&gt; if you see any blockers.\n```",       "type": "text",       "id": "Field_1p84mr7"     },     {       "label": "Release version",       "type": "textfield",       "id": "Field_0roc0io",       "key": "release_version"     },     {       "label": "Code freeze date",       "type": "textfield",       "id": "Field_07bgspy",       "key": "code_freeze_date"     },     {       "label": "Release Manager",       "type": "textfield",       "id": "Field_0r6ogpl",       "key": "release_manager"     }   ],   "type": "default",   "id": "notify_code_freeze_from",   "executionPlatform": "Camunda Cloud",   "executionPlatformVersion": "1.2" }</zeebe:userTaskForm>
